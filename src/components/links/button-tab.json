{
  "collectionName": "components_tab_buttons",
  "info": {
    "name": "Button-tab",
    "displayName": "Tab",
    "icon": "fingerprint",
    "description": ""
  },
  "options": {},
  "attributes": {
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^$|^[a-zA-Z/-]+$"
    },
    "text": {
      "type": "string"
    },
    "type": {
      "type": "enumeration",
      "enum": ["button", "link", "image"],
      "default": "link"
    },
    "image": {
      "allowedTypes": ["images"],
      "type": "media",
      "multiple": false
    },
    "richtext": {
      "type": "customField",
      "options": {
        "preset": "toolbarBaloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "video": {
      "type": "component",
      "repeatable": false,
      "component": "sections.large-video"
    },
    "googleMap": {
      "type": "component",
      "repeatable": false,
      "component": "elements.gmap"
    }
  }
}
